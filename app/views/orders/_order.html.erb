<tr>
  <td width="15%"><%= order.id %> </td>
  <td width="15%"><%= order.rest_id %> </td>
  <td width="15%"><%= order.user_id %></td>
  <td width="20%"><%= order.total %></td>
  <td width="20%">
    <% if order.payment_type == 1 %>
      Credit Card
    <% else %>
      Cash
    <% end %>
  </td>
  <td width="15%"><%= order.note %></td>
</tr>