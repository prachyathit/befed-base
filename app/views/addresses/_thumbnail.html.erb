<div class="col-sm-6 address-thumb">
  <div class="panel panel-default">
    <div class="panel-heading">
      Name
      <div class="pull-right">
        <%= link_to "", new_address_path,  { :class=>"glyphicon glyphicon-pencil" } %>
        <a  href="#"><span class="glyphicon glyphicon-trash"></span></a>
      </div>
      <div class="clearfix;"></div>
    </div>
    <div class="panel-body">
      <div>address</div>
      <div>
        <div id="googleMap" class="thumb-map">
          <%= render partial: "shared/address", locals: {latitude: '13.8894791', longitude: '100.56435499999998', elementId: 'googleMap'} %>
        </div>
      </div>
    </div>
  </div>
</div>